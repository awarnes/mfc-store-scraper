# Azure API
APP_ID=your_app_id
API_KEY=your_api_key

# Top Level Categories
TOP_LEVEL_CATEGORIES={"Household & Family": 21924, "Food": 21244, "Health & Beauty": 19457, "Outdoor & Garden": 19159, "Nutritional Supplements": 17257}
