ENVIRONMENT=local

# Logging (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Database
DB_HOST=localhost
DB_NAME=azure
DB_PORT=5432
DB_USERNAME=root
DB_PASSWORD=your_password

# For local development when we spin up a fresh postgres container
